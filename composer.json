{
    "name": "norman-huth/nova-menu",
    "description": "This package brings back the policy authorization for MenuItem. Also, the well-known canSee method is added to MenuItem and MenuSection. Empty menu sections are automatically hidden.",
    "homepage": "https://github.com/Muetze42/nova-menu",
    "support": {
        "issues": "https://github.com/Muetze42/nova-menu/issues",
        "source": "https://github.com/Muetze42/nova-menu",
        "docs": "https://github.com/Muetze42/nova-menu#readme"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Norman Huth",
            "homepage": "https://huth.it"
        }
    ],
    "keywords": [
        "nova",
        "laravel",
        "nova-4",
        "laravel-nova"
    ],
    "autoload": {
        "psr-4": {
            "NormanHuth\\NovaMenu\\": "src/"
        }
    },
    "require": {
        "laravel/nova": "^4.0.0",
        "php": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NormanHuth\\NovaMenu\\PackageServiceProvider"
            ]
        }
    }
}
